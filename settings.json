{
  "workbench.colorCustomizations": {
    "activityBar.background": "#037d4a",
    "activityBar.foreground": "#fffffe",
    "activityBar.inactiveForeground": "#bbb3ac"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.requireConfig": true,
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.associations": {
    "Fastfile*": "ruby",
    "Appfile": "ruby",
    "Matchfile": "ruby",
    "package-lock.lock": "text",
    ".env*": "dotenv",
    "package.json": "jsonc"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "never",
    "source.fixAll.eslint": "explicit"
  },
  "spellright.language": ["en"],
  "spellright.documentTypes": ["markdown", "latex", "plaintext"],
  "files.readonlyInclude": {
    "app/translations/**/*": true
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.test.ts, ${capture}.spec.ts, ${capture}.test.ts.snap",
    "*.tsx": "${capture}.test.tsx, ${capture}.spec.tsx, ${capture}.test.tsx.snap",
    ".eslintrc.js": "*.eslintrc.js",
    "app.config.ts": "*.config.js"
  },
  "editor.semanticTokenColorCustomizations": {}
}
